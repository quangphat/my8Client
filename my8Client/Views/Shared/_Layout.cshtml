<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - my8.Client</title>
    <base href="~/" />

    <link rel="stylesheet" href="~/dist/vendor.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/assets/styles//bootstrap.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/assets/fonts/font-awesome-4.7.0/css/font-awesome.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/assets/styles/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/assets/styles/mobile.css" asp-append-version="true" />
</head>
<body style="background-color:#e9ebee">
    @RenderBody()

    <script src="~/dist/vendor.js" asp-append-version="true"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
